{
  "preset": "psr12",
  "rules": {
    "concat_space": {
      "spacing": "one"
    },
    "class_attributes_separation": {
      "elements": {
        "const": "none",
        "method": "one",
        "property": "one",
        "trait_import": "none"
      }
    },
    "trailing_comma_in_multiline": true
  }
}
